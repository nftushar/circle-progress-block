{
  "apiVersion": "1",
  "name": "b-blocks/circle-progress",
  "title": "Circle Progress",
  "description": "Gutenberg editing with easy-to-use, responsive circle-progress  more.",
  "category": "media",
  "keywords": ["circle-progress", "Circle", "Progress", "Gutenberg"],
  "textdomain": "circle-progress",

  "attributes": {
    "align": {
      "type": "string",
      "default": ""
    },
    "cId": {
      "type": "string",
      "default": ""
    },
    "value": {
      "type": "number",
      "default": 0.75
    },
    "size": {
      "type": "number",
      "default": 100
    },
    "thickness": {
      "type": "number",
      "default": 7
    },
    "lineCap": {
      "type": "string",
      "default": "butt"
    },
    "startAngle": {
      "type": "number",
      "default": 0
    },
    "reverse": {
      "type": "boolean",
      "default": true
    },
    "animation": {
      "type": "object",
      "default": {
        "enabled": true,
        "duration": 1200,
        "startAt": 0
      }
    },
    "elements": {
      "type": "object",
      "default": {
        "percentage": true
      }
    },
    "alignment": {
      "type": "string",
      "default": "center"
    }, 
    "fill": {
      "type": "object",
      "default": {
        "type": "string",
        "color":"linear-gradient(135deg,#12c2e9 0%,#c471ed 50%,#f64f59 100%)"
      }
    },
    "emptyFill": {
      "type": "string",
      "default": "#000"
    }
  },
  "example": {
    "attributes": {
      "preview": true,
      "columns": {
        "desktop": 1,
        "tablet": 1,
        "mobile": 1
      }
    }
  },
  "editorScript": "file:../dist/editor.js"
}
